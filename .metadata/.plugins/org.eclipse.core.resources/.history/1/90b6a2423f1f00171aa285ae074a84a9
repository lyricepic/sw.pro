package P0412;

import java.io.FileInputStream;
import java.util.Scanner;

public class C {
	
	static int N;
	static char[] TEXT;
	static char[][] M;
	static int[][] vis;
	static int rst = 0;
	
	public static void main(String[] args) throws Exception{
		
		System.setIn(new FileInputStream("test.txt"));
		Scanner sc = new Scanner(System.in);
		
		int T = sc.nextInt();
		int test_case;
		
		for(test_case = 1; test_case <= T; test_case++){
			// 입력부
			N = sc.nextInt();
			TEXT = new char[N];
			M = new char[N][N];
			vis = new int[N][N];
			rst = 0;
			
			for(int i = 0; i <= N; i++){
				String strCard = sc.next();
				for(int j = 0; j < N; j++){
					if(i == 0){
						TEXT[j] = strCard.charAt(j);
					}else{
						M[i-1][j] = strCard.charAt(j);		
					}
				}
			}			
			// 솔루션
			for(int i = 0; i < N; i++){
				for(int j = 0; j < N; j++){
					f(i ,j, 0);
				}
			}

			// 출력부
			System.out.println("#" + test_case + " " + rst);
		}
	}
	
	static void f(int r, int c, int n){
		
		if(n == N){
			rst = 1;
			return;
		}
		
		if(c+1 < N && M[r][c+1] == TEXT[n] && vis[r][c+1] == 0){
			vis[r][c+1] = 1;
			f(r, c+1, n+1);
			vis[r][c+1] = 0;
		}
		if(r+1 < N && M[r+1][c] == TEXT[n] && vis[r+1][c] == 0){
			vis[r+1][c] = 1;
			f(r+1, c, n+1);
			vis[r+1][c] = 0;
		}
		if(c-1 >= 0 && M[r][c-1] == TEXT[n] && vis[r][c-1] == 0){
			vis[r][c-1] = 1;
			f(r, c-1, n+1);
			vis[r][c-1] = 0;
		}
		if(r-1 >= 0 && M[r-1][c] == TEXT[n] && vis[r-1][c] == 0){
			vis[r-1][c] = 1;
			f(r-1, c, n+1);
			vis[r-1][c] = 0;
		}
	}
}
