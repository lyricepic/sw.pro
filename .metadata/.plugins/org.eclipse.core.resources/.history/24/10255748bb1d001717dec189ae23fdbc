package Practice;

import java.io.FileInputStream;
import java.util.Scanner;

public class E {
	
	static int N;
	static int[][] M;
	static int[] visited;
	static int sum;
	static int min;
	
	public static void main(String[] args) throws Exception{
		
		System.setIn(new FileInputStream("test.txt"));
		Scanner sc = new Scanner(System.in);
		
		int T = sc.nextInt();
		int test_case;
		
		for(test_case = 1; test_case <= T; test_case++){
			// 입력부
			N = sc.nextInt();
			M = new int[N][N];
			visited = new int[N];
			sum = 0;
			min = Integer.MAX_VALUE;
			
			for(int i = 0; i < N; i++){
				for(int j = 0; j < N; j++){
					M[i][j] = sc.nextInt();
				}
			}
			
			// 솔루션
			f(0, 0);
			
			// 출력부
			System.out.println("#" + test_case + " " + min);
		}
	}
	
	static void f(int n, int k){
		
		if(n == N)
			return;
		
		if(k == N+1){
			min = min > sum ? sum : min;
			return;
		}
		
		for(int i = 0; i < N; i++){
			if(i == 0 && k == 0){
				visited[i] = 1;
				sum += M[n][i];
				f(n+1, k+1);
			}else if(k == N && i != 0){
				break;
			}else if(i == 0){
				break;
			}else{
				if(M[n][i] != 0 && visited[i] != 1){
					visited[i] = 1;
					sum += M[n][i];
					f(n, k+1);	
				}				
			}
			
			visited[i] = 0;
		}
	}
}
