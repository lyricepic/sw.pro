package P0414;

import java.io.FileInputStream;
import java.util.Scanner;

import P0413.Pt;

public class B {
	
	static int N;
	static int E;
	static int[][] M;
	
	public static void main(String[] args) throws Exception{
		
		System.setIn(new FileInputStream("test.txt"));
		Scanner sc = new Scanner(System.in);
		
		int T = sc.nextInt();
		int test_case;
		
		for(test_case = 1; test_case <= T; test_case++){
			// INPUT
			N = sc.nextInt();
			E = sc.nextInt();
			M = new int[N+1][N+1];
			
			for(int i = 0; i <= E; i++){
				int s = sc.nextInt();
				int d = sc.nextInt();
				int w = sc.nextInt();
				
				M[s][d] = w;
			}
			
			// SOLUTION
			
			
			// OUT
			System.out.println("#" + test_case);
		}
	}
	
	static void f(){
		
	}
}

class Pt implements Comparable<Pt>{

	int r;
	int c;
	int w;
	
	Pt(int r, int c, int w){
		this.r = r;
		this.c = c;
		this.w = w;
	}
	
	@Override
	public int compareTo(Pt o) {
		if(this.w > o.w)
			return -1;
		else if(this.w < o.w)
			return 1;
		else{
			return 0;		
		}
	}
}

