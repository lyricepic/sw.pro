package study;

public class Permutaion {
	
	static int K;
	static int[] A;
	static int[] C;
	
	public static void main(String[] args) {
		// input
		
	}
	
	static void f(int n, int k){
		
		if(k == 0){
			// output
			for(int i = 0; i < K; i++){
				System.out.println(C[i]);
			}
			return;
		}else if(n < k){
			return;		
		}else{
			C[k-1] = A[n-1];
			f(n-1, k-1);
			f(n-1, k);
		}
	}
}
